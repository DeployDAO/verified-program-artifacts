          {
            "name": "ExceededMaxClaim"
          },
          {
            "name": "ExceededMaxNumNodes"
          },
          {
            "name": "Unauthorized"
          },
          {
            "name": "OwnerMismatch"
          }
        ]
      }
    }
  ],
  "events": [
    {
      "name": "ClaimedEvent",
      "fields": [
        {
          "name": "index",
          "type": "u64",
          "index": false
        },
        {
          "name": "claimant",
          "type": "publicKey",
          "index": false
        },
        {
          "name": "amount",
          "type": "u64",
          "index": false
